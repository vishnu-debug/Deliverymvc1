@model IEnumerable<Deliverymvc1.Models.New>
    <partial name="_Executivepartial" />

@*<h1> You sucessfully Accepted that request </h1>
    @model IEnumerable<Deliverymvc1.Models.New>*@
<body style="background-color:powderblue;">

<h2> Request Accepted/Rejected</h2>
@foreach (var item in Model)
{

    if (item.Status == true)
    {
        <h6>Request ID: @Html.DisplayFor(modelItem => item.ExrequestID)</h6>
        <h1> You sucessfully Accepted that request </h1>
    }

    else
    {
        <h6>Request ID: @Html.DisplayFor(modelItem => item.ExrequestID)</h6>
        <h4> You sucessfully Rejected that request </h4>
    }
}
